    <versionUuid>_a-0000e71b-9358-8000-6ea4-01ef9001ef90_23249</versionUuid>
    <rule>
        <name>BID_decisionSummary</name>
        <uuid>_a-0000d4ae-e38c-8000-ac7a-680000680000_4095</uuid>
        <description>This summarizes the bidding results to be displayed in approval task names.</description>
        <parentUuid>_a-0000d4ae-e38c-8000-ac7a-680000680000_4090</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=length(wherecontains("Bid", ri!decisionResults)) &amp; "Y" &amp; length(wherecontains("No Bid", ri!decisionResults)) &amp; "N"</definition>
        <namedTypedValue>
            <name>decisionResults</name>
            <type>
                <name>Text?list</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
