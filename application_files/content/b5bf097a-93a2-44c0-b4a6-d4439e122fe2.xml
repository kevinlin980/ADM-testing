<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e71b-9358-8000-6ea4-01ef9001ef90_25314</versionUuid>
    <rule>
        <name>reportDocumentBrowserFieldColumns</name>
        <uuid>b5bf097a-93a2-44c0-b4a6-d4439e122fe2</uuid>
        <description>tests Miller columns layout</description>
        <parentUuid>_a-0000d6fd-7cb9-8000-7886-010000010000_951</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>
load(
  local!knowledgecenter: fn!contentuuidtoid_appian_internal("SYSTEM_KC_SYSTEM_KNOWLEDGE_CENTER"),
  local!folderId: null,
  local!sel,
  local!nav,
  #"SYSTEM_SYSRULES_dashboardLayout"(
    firstColumnContents:{
      #"SYSTEM_SYSRULES_pickerFieldFolders"(
        label: "Select Folder",
        labelPosition: "ADJACENT",
        maxSelections: 1,
        value: local!folderId,
        saveInto: local!folderId,
        folderFilter: fn!tointeger(local!knowledgecenter)
      ),
      if(
        #"SYSTEM_SYSRULES_isNullOrEmpty"(local!knowledgecenter),
        {},
        #"SYSTEM_SYSRULES_documentBrowserFieldColumns_v1"(
          knowledgecenter: if(#"SYSTEM_SYSRULES_isNullOrEmpty"(local!folderId), local!knowledgecenter, null),
          folder: local!folderId,
          height: "TALL",
          navigationValue: local!nav,
          navigationSaveInto: local!nav,
          selectionValue: local!sel,
          selectionSaveInto: local!sel
        )
      )
    }
  )
)
        </definition>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role inherit="true" allowForAll="false" name="readers">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="authors">
            <users/>
            <groups/>
        </role>
        <role inherit="true" allowForAll="false" name="administrators">
            <users>
                <userUuid>Administrator</userUuid>
            </users>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyReaders">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAuthors">
            <users/>
            <groups/>
        </role>
        <role inherit="false" allowForAll="false" name="denyAdministrators">
            <users/>
            <groups/>
        </role>
    </roleMap>
    <history>
        <historyInfo versionUuid="_a-0000e71b-9358-8000-6ea4-01ef9001ef90_25314"/>
    </history>
</contentHaul>
