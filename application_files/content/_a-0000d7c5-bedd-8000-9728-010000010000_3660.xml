<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <versionUuid>_a-0000e71b-9358-8000-6ea4-01ef9001ef90_24220</versionUuid>
    <rule>
        <name>createGridColumnForID</name>
        <uuid>_a-0000d7c5-bedd-8000-9728-010000010000_3660</uuid>
        <description>This will either create a TextColumn or a LinkColumn</description>
        <parentUuid>_a-0000d7c5-bedd-8000-9728-010000010000_1493</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=
if(
 and(ri!label = "ID", not(isnull(ri!data))),

 #"SYSTEM_SYSRULES_gridLinkColumn"(
   label: ri!label,
   field: ri!field,
   data: apply(#"SYSTEM_SYSRULES_processTaskLink"(task:_, label:"Open Task"), ri!data),
   alignment: ri!alignment
  )
  ,
  #"SYSTEM_SYSRULES_gridLinkColumn"(
    label: ri!label,
    field: ri!field,
    data: ri!data,
    alignment: ri!alignment
  )
)</definition>
        <namedTypedValue>
            <name>label</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>field</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>data</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>alignment</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
