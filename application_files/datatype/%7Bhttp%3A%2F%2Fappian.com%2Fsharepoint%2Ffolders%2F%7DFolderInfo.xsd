<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://appian.com/sharepoint/folders/"
  xmlns:tns="http://appian.com/sharepoint/folders/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:complexType name="FolderInfo">
